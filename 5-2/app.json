{
    "expo": {
        "name": "Tech Track",
        "slug": "techtrack",
        "description": "Track you field operation team using Tech Track",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/you.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/you.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/images/you.png"
        }
    }
}
